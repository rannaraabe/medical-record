<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ProntuariosController">
   <children>
      <TextArea fx:id="taResultado" layoutX="13.0" layoutY="59.0" prefHeight="423.0" prefWidth="675.0" promptText="                                                                                 # Prontuários #">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font></TextArea>
      <Button fx:id="btProntuarios" layoutX="14.0" layoutY="18.0" mnemonicParsing="false" onAction="#selecionarProntuarios" text="Selecionar prontuários">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font></Button>
      <Button fx:id="btVoltar" layoutX="659.0" layoutY="18.0" mnemonicParsing="false" onAction="#voltarTela" text="←">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font></Button>
      <Button fx:id="btMetricas" layoutX="521.0" layoutY="18.0" mnemonicParsing="false" onAction="#selecionarMetricas" text="Selecionar métricas">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
