<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="700.0" maxWidth="1000.0" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ProntuariosController">
   <children>
      <Button fx:id="btVoltar" layoutX="16.0" layoutY="17.0" mnemonicParsing="false" onAction="#voltarTela" text="←">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="btProntuarios" layoutX="804.0" layoutY="17.0" mnemonicParsing="false" onAction="#selecionarProntuarios" text="Selecionar prontuários">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="btMetricas" layoutX="343.0" layoutY="658.0" mnemonicParsing="false" onAction="#selecionarMetricas" text="Selecionar métricas">
         <font>
            <Font size="14.0" />
         </font></Button>
      <TableView fx:id="tvResultado" layoutX="16.0" layoutY="53.0" prefHeight="593.0" prefWidth="967.0">
        <columns>
          <TableColumn fx:id="colunaArquivo" prefWidth="355.0" text="Arquivo" />
          <TableColumn fx:id="colunaCaminho" prefWidth="611.0" text="Caminho" />
        </columns>
      </TableView>
      <Button fx:id="btSimilaridade" layoutX="508.0" layoutY="658.0" mnemonicParsing="false" onAction="#gerarSimilaridade" text="Gerar similaridade">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
