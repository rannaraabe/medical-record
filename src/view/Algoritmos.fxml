<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AlgoritmosController">
   <children>
      <Button fx:id="btVoltar" layoutX="658.0" layoutY="21.0" mnemonicParsing="false" onAction="#voltarTela" text="←">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btResultado" layoutX="293.0" layoutY="301.0" mnemonicParsing="false" onAction="#gerarResultado" text="Gerar resultados">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Button>
      <Label fx:id="lbSelecionarAlgoritmos" layoutX="281.0" layoutY="122.0" text="Selecionar algoritmos:">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font>
      </Label>
      <CheckBox fx:id="cbCosine" layoutX="281.0" layoutY="157.0" mnemonicParsing="false" onAction="#selecionarCosine" text="Cosine">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="cbTrigram" layoutX="281.0" layoutY="186.0" mnemonicParsing="false" onAction="#selecionarTrigram" text="Trigram">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="cbLevenshtein" layoutX="281.0" layoutY="216.0" mnemonicParsing="false" onAction="#selecionarLevenshtein" text="Levenshtein">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="cbJaroWinkler" layoutX="281.0" layoutY="245.0" mnemonicParsing="false" onAction="#selecionarJaroWinkler" text="JaroWinkler">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
