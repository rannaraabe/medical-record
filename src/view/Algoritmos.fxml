<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="500.0" maxWidth="700.0" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AlgoritmosController">
   <children>
      <Button fx:id="btVoltar" layoutX="19.0" layoutY="18.0" mnemonicParsing="false" onAction="#voltarTela" text="←">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font></Button>
      <Label fx:id="lbSelecionarAlgoritmos" layoutX="288.0" layoutY="18.0" text="Selecionar métricas:">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font></Label>
      <CheckBox fx:id="cbCosine" layoutX="260.0" layoutY="43.0" mnemonicParsing="false" text="Cosine">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font></CheckBox>
      <CheckBox fx:id="cbTrigram" layoutX="259.0" layoutY="69.0" mnemonicParsing="false" text="Trigram">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font></CheckBox>
      <CheckBox fx:id="cbLevenshtein" layoutX="342.0" layoutY="44.0" mnemonicParsing="false" text="Levenshtein">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font></CheckBox>
      <CheckBox fx:id="cbJaroWinkler" layoutX="342.0" layoutY="70.0" mnemonicParsing="false" text="JaroWinkler">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font></CheckBox>
      <Button fx:id="btImportar" layoutX="342.0" layoutY="454.0" mnemonicParsing="false" onAction="#exportarResultados" text="Exportar resultados">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font></Button>
      <TextArea fx:id="taResultados" layoutX="19.0" layoutY="299.0" prefHeight="146.0" prefWidth="662.0" promptText="Resultados" wrapText="true">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font></TextArea>
      <Button fx:id="btResultado" layoutX="233.0" layoutY="454.0" mnemonicParsing="false" onAction="#verResultados" text="Ver resultados">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Button>
      <TextArea fx:id="taProntuarios" layoutX="20.0" layoutY="102.0" prefHeight="192.0" prefWidth="662.0">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </TextArea>
   </children>
</AnchorPane>
